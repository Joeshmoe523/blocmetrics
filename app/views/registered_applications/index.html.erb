<p id="notice"><%= notice %></p>

<h1>Listing Registered Applications</h1>
<button id="awesome">Button we want to track</button>

<script>
  $("#awesome").click(function(){
    $.post("http://localhost:3001/events", {event: {name: "Awesome Button Click"}})
  })

</script>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: "registered_applications/registered_applications", locals: { registered_applications: @registered_applications } %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered application', new_registered_application_path %>
